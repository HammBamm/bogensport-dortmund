{
  "name": "bogensport-dortmund-server",
  "version": "1.0.0",
  "engines": {
    "node": "14.17.2",
    "npm": "7.21.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "express-sslify": "^1.2.0",
    "nodemailer": "^6.6.3"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.1"
  }
}